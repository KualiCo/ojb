  <build>
    <sourceDirectory>${basedir}/target/src</sourceDirectory>
    <plugins>
      <plugin>
        <artifactId>maven-jar-plugin</artifactId>
        <configuration>
          <excludes>
            <param>MANIFEST.MF</param>
            <param>ojb-jca-service.xml</param>
            <param>ra.xml</param>
          </excludes>
        </configuration>
      </plugin>
    </plugins>
  </build>
