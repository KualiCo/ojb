<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <parent>
    <groupId>org.kuali</groupId>
    <artifactId>kuali</artifactId>
    <version>71</version>
  </parent>
  <groupId>org.kuali.ojb</groupId>
  <artifactId>db-ojb</artifactId>
  <version>1.0.4-patch5-SNAPSHOT</version>
  <name>ObJectRelationalBridge</name>
  <description>
    ObJectRelationalBridge (OJB) is an Object/Relational mapping tool that
    allows transparent persistence for Java Objects against relational
    databases.
  </description>
  <repositories>
    <repository>
      <id>kuali.nexus</id>
      <name>Kuali Nexus Repository Manager</name>
      <url>http://nexus.kuali.org/content/groups/public</url>
      <snapshots>
        <enabled>true</enabled>
      </snapshots>
      <releases>
        <enabled>true</enabled>
      </releases>
    </repository>
  </repositories>
  <scm>
    <connection>scm:svn:https://test.kuali.org/svn/kul-obj-1.0.4/trunk</connection>
    <developerConnection>scm:svn:https://test.kuali.org/svn/kul-obj-1.0.4/trunk</developerConnection>
    <url>https://test.kuali.org/svn/kul-obj-1.0.4/trunk</url>
  </scm>
  <properties>
    <kuali.clover.generate.info.phase>none</kuali.clover.generate.info.phase>
    <db-ojb.jdk.version>1.5</db-ojb.jdk.version>
    <kuali.compile.target>${db-ojb.jdk.version}</kuali.compile.target>
    <kuali.compile.source>${db-ojb.jdk.version}</kuali.compile.source>
    <kuali.javadoc.report.2>javadoc</kuali.javadoc.report.2>
    <kuali.javadoc.tool.version>${db-ojb.jdk.version}</kuali.javadoc.tool.version>
    <maven.test.skip>true</maven.test.skip>
    <kuali.clover.exclude.pattern.1>**/*.java</kuali.clover.exclude.pattern.1>
  </properties>
  <dependencies>
    <!-- 
    <dependency>
      <id>ejb</id>
      <version>1.0</version>
      <jar>ejb.jar</jar>
      <url>http://java.sun.com/j2ee/</url>
